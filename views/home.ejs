<!DOCTYPE html>
<html>

<head>
   <title>Povjest Narudžbi</title>
   <meta charset="utf-8">
   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>

<body>
   <div class="container mt-3" style="width: 60%;">
      <h2 class="text-center">Povjest Naruđbi</h2>
      <table class="table table-bordered">
         <thead>
            <tr>
               <th>Guma</th>
               <th>Mjesto narucivanja</th>
               <th>UF</th>
               <th>Dolazak</th>
               <th>Naručeno</th>
            </tr>
         </thead>
         <tbody>
            <% for(const mail of mail_list){ %>
            <tr>
               <td class="tyre">
                  <%= mail.tyre %>
               </td>
               <td class="supplier">
                  <%=mail.supplier  %>
               </td>
               <td class="uf">
                  <%=mail.uf  %>
               </td>
               <td class="date">
                  <%=mail.date  %>
               </td>
               <td class="arrival">
                  <%=mail.arrival  %>
               </td>
            </tr>
            <% } %>
         </tbody>
      </table>
   </div>
</body>

</html>